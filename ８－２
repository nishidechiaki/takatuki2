    12.0: {0.01: -0.007916233012934112, 0.015: -0.007853313547945847, 0.02: -0.00779557069308283, 0.025: -0.00774365783060699, 0.03: -0.007697627177721247, 0.035: -0.007658003212820958, 0.04: -0.00762506239360031, 0.045: -0.007598999327783976, 0.05: -0.0075799659687684415}
}

# rtの値ごとに異なる色でプロット
colors = ['b', 'g', 'r', 'c', 'm', 'y', 'k', 'orange', 'purple']
rt_values = [0.01, 0.015, 0.02, 0.025, 0.03, 0.035, 0.04, 0.045, 0.05]

plt.figure(figsize=(10, 6))

for i, rt in enumerate(rt_values):
    L_values = sorted(results.keys())
    ramuda_values = [results[L][rt] for L in L_values]
    
    # スムーズな曲線を描くためにスプライン補間を使用
    spl = make_interp_spline(L_values, ramuda_values, k=3)
    L_smooth = np.linspace(min(L_values), max(L_values), 500)
    ramuda_smooth = spl(L_smooth)
    
    plt.plot(L_smooth, ramuda_smooth, color=colors[i], label=f'rt={rt:.3f}')

plt.xlabel('L')
plt.ylabel('ramuda')
plt.title('Smooth Curve for Different rt Values')
#plt.xlim(0, 12)
#plt.ylim(-0.01, 0)
plt.legend()
plt.grid(True)
plt.show()
