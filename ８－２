    10.0: {0.01: -0.0008512236956066098, 0.015: -0.00087828759452697, 0.02: -0.0009053514934473303, 0.025: -0.0009324153923676904, 0.03: -0.0009594792912880508, 0.035: -0.000986543190208411, 0.04: -0.0010136070891287713, 0.045: -0.0010406709880491317, 0.05: -0.0010677348869694919},
